let password = 'пароль';
prompt('Введите пароль');
if (password == пароль) {
    alert('Пароль введен верно');
} else {
    alert('Пароль введен неправильно')
}

let c = 2;
if (c > 0 && c < 10) {
    alert('Верное число');
} else {
    alert('Число неверно')
}

let d = 56;
let e = 35;
if (d > 100 || e > 100) {
    alert('Верно')
} else {
    alert('Неверно')
}

